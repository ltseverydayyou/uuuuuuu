if game.GameId == 5120885191 then
	local input = game:GetService("UserInputService")
	local p = game:GetService("Players").LocalPlayer
	local gui = p:WaitForChild("PlayerGui")
	getgenv().visualizer = true
	loadstring(game:HttpGet("https://raw.githubusercontent.com/1f0yt/community/main/RedCircleBlock"))()

	loadstring(game:HttpGet("https://raw.githubusercontent.com/ltseverydayyou/uuuuuuu/main/keyboard"))()
	wait(0.65)
	game.CoreGui["Keyboard gui"].Open.Visible = false
	game.CoreGui["Keyboard gui"].drag1.Visible = true
	game.CoreGui["Keyboard gui"].drag1.Size = UDim2.new(0.5,0,0.1,0)
	game.CoreGui["Keyboard gui"].drag1.Frame1.BackgroundTransparency = 1
	game.CoreGui["Keyboard gui"].drag1.Frame1.X.Text = "Spam"
	game.CoreGui["Keyboard gui"].drag1.BackgroundTransparency = 1
	game.CoreGui["Keyboard gui"].drag1.Frame1.X.Position = game.CoreGui["Keyboard gui"].drag1.Position + UDim2.new(-0.05,0,-0.1,0)
	game.CoreGui["Keyboard gui"].drag1["Hide/sho"].Visible = false
	game.CoreGui["Keyboard gui"].drag1.Miniize1.Visible = false
	game.CoreGui["Keyboard gui"].drag1.Close1.Visible = false

	local spampos = Instance.new("TextButton")
	spampos.Name = "position"
	spampos.Text = ""
	spampos.Size = UDim2.new(0, 75, 0, 55)
	spampos.Position = UDim2.new(0.5, -205, 0.8, -25)
	spampos.Transparency=1

	for i, v in pairs(game.CoreGui["Keyboard gui"].drag1.Frame1:GetDescendants()) do
		if v.Name ~= "X" then
			v.Visible = false
		end
	end


	local screenGui = Instance.new("ScreenGui")
	screenGui.IgnoreGuiInset = true
	screenGui.Parent = gui
	screenGui.ResetOnSpawn = false
	screenGui.Name = math.random(1,69420)

	local spamButton = game.CoreGui["Keyboard gui"].drag1.Frame1.X
	spamButton.Text = "Spam"
	spamButton.Parent=screenGui
	spamButton.Size = spampos.Size
	spamButton.Position=spampos.Position
	game.CoreGui["Keyboard gui"].Enabled=false

	local parryButton = Instance.new("TextButton")
	parryButton.Name = "Parry"
	parryButton.Text = "Parry"
	parryButton.Size = UDim2.new(0, 75, 0, 55)
	parryButton.Position = UDim2.new(0.5,155 , 0.8, -25)
	parryButton.Parent = screenGui

	parryButton.MouseButton1Down:Connect(function()
		local args = {
			[1] = "SwordService",
			[2] = "Block",
			[3] = {
				[1] = -0.5340168476104736
			}
		}

		game:GetService("ReplicatedStorage").Framework.RemoteFunction:InvokeServer(unpack(args))

	end)

	local spamming = false

	local function spam()
		while spamming do
			for i=1, 3 do
				local args = {
					[1] = "SwordService",
					[2] = "Block",
					[3] = {
						[1] = -0.5340168476104736
					}
				}

				game:GetService("ReplicatedStorage").Framework.RemoteFunction:InvokeServer(unpack(args))
			end
			task.wait()
		end
	end

	spamButton.MouseButton1Down:Connect(function()
		spamming = not spamming
		if spamming then
			spamButton.Text = "Stop"
			spawn(spam)
		else
			spamButton.Text = "Spam"
		end
	end)

	function ballz(bell)
		wait(.4)
		for i, v in pairs(bell:GetDescendants()) do
			if v:IsA("ParticleEmitter")  then
				v:Destroy()
			end
		end
		bell.Transparency=0
		local balls1 = game.ReplicatedStorage.Storage.Balls
		if bell.Color == balls1.Ball1.Color then
			local highlight = Instance.new("SelectionBox")
			highlight.Color3 = bell.Color
			highlight.Adornee = bell
			highlight.Parent = bell
			highlight.Name = "ball1"
			game.StarterGui:SetCore("SendNotification",  {
				Title = "Ball Spawned";
				Text = "Ball Name: " ..bell.Name;
				Icon = "";
				Duration = 1;
				Callback = NotificationBindable;
			})
		elseif bell.Color == balls1.UltimateBall1.Color then
			bell.Name = "UltimateBall"
			local highlight = Instance.new("SelectionBox")
			highlight.Color3 = bell.Color
			highlight.Adornee = bell
			highlight.Parent = bell
			highlight.Name = "ultimateball1"
			game.StarterGui:SetCore("SendNotification",  {
				Title = "Ultimate Ball Spawned";
				Text = "Ultimate Ball Name: " ..bell.Name;
				Icon = "";
				Duration = 2;
				Callback = NotificationBindable;
			})
		end
	end

	game.workspace.Balls.ChildAdded:Connect(ballz)
	wait(1)
	game.Workspace.Effects:Remove()

	getgenv().ballzui = true;

	while getgenv().ballzui and task.wait() do
		pcall(function()
			local ui = p.PlayerGui.HUD
			if input.TouchEnabled then
				ui.MobileLarge.Visible = true
				ui.MobileSmall.Visible = false
				ui.Actions.Visible = false
				ui.MobileLarge.Block.Visible = false
			else
				ui.MobileLarge.Visible = false
				ui.MobileSmall.Visible = false
				ui.Actions.Visible = true
			end                        
		end)
	end
end
